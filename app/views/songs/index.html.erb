<p id="notice"><%= notice %></p>

<h1>Listing Songs</h1>

<table>
  <thead>
    <tr>
      <th>Cod song</th>
      <th>Nom song</th>
      <th>Artista song</th>
      <th>Genero song</th>
      <th>Anio song</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.cod_song %></td>
        <td><%= song.nom_song %></td>
        <td><%= song.artista_song %></td>
        <td><%= song.genero_song %></td>
        <td><%= song.anio_song %></td>
        <td><%= song.estado %></td>
        <td><%= link_to 'Show', song %></td>
        <td><%= link_to 'Edit', edit_song_path(song) %></td>
        <td><%= link_to 'Destroy', song, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Song', new_song_path %>
